  "bugs": {
    "url": "https://github.com/Sam54123/rl_among_us/issues"
  },
  "homepage": "https://github.com/Sam54123/rl_among_us#readme",
  "dependencies": {
    "@types/socket.io": "^2.1.11",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "socket.io": "^3.0.0"
  },
  "devDependencies": {
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.6",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
